{
    "name": "laravolt/semantic-form",
    "description": "Semantic-UI form helpers",
    "keywords": [
        "laravel",
        "laravolt",
        "form",
        "semantic-UI"
    ],
    "homepage": "https://github.com/laravolt/semantic-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.2",
        "nesbot/carbon": "~1.20",
        "jenssegers/date": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\SemanticForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\SemanticForm\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\SemanticForm\\ServiceProvider"
            ],
            "aliases": {
                "SemanticForm": "Laravolt\\SemanticForm\\Facade"
            }
        }
    }
}
